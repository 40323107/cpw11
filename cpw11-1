symbolDict=<br>{"4":symbol1,"0":symbol2,"3":symbol3,"2":symbol4,"3":symbol5,"1":symbol6,<br>"0":symbol7,"7":symbol8,"S":symbol9,"A":symbol10,"N":symbol11,"D":symbol12,<br>"Y":symbol13,"E":symbol14,"N":symbol15,"D":symbol16}
 
row=9
i=1;
while i:
inp=input("enter a string to display or 'exit' to end program:")
if inp == "OVER":
i=0
break
else:
for r in range(row):
for c in inp:
print(symbolDict[c](r),end=' ')
print()
